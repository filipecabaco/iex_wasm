[UNDEFINED] enter    [IF]
[UNDEFINED] E.LIBERR [IF] include lib/throw.4th [THEN]

: enter
  begin
    refill 0= E.LIBERR throw" EOF on stdin"
    bl parse-word                      ( a n)
    number error?                      ( n f)
  while                                ( n)
    drop                               ( --)
  repeat                               ( n)
;
[THEN]
