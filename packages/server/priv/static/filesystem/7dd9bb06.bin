[UNDEFINED] fenter  [IF]
[UNDEFINED] >float  [IF] [ABORT] [THEN]
[UNDEFINED] E.LIBERR [IF] include lib/throw.4th [THEN]

: fenter
  begin
    refill 0= E.LIBERR throw" EOF on stdin"
    bl parse-word >float
  until
;
[THEN]
